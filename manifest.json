{
  "manifest_version": 2,
  "name": "web request logger example",
  "version": "1.0",

  "browser_action": {
    "default_icon": "clock.png"
},

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "idle",
    "alarms",
    "tts"
  ],

  "web_accessible_resources": [

  ]
}